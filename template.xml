    <rate>
      <timebase>30</timebase>
      <ntsc>false</ntsc>
    </rate>

    <media>
      <video>
        <track>
          {clipitems}
        </track>
      </video>
    </media>
  </sequence>
  <marker></marker>
</xmeml>